{
    "version": 2,
    "builds": [
        {
            "src": "api/market.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/market",
            "dest": "api/market.py"
        },
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "headers": [
        {
            "source": "/api/(.*)",
            "headers": [
                {
                    "key": "Access-Control-Allow-Origin",
                    "value": "*"
                },
                {
                    "key": "Access-Control-Allow-Methods",
                    "value": "GET, OPTIONS"
                },
                {
                    "key": "Access-Control-Allow-Headers",
                    "value": "Content-Type"
                }
            ]
        }
    ]
}